{
  "metadata": [
    {
      "outputFormat": "markdown",
      "categoryLayout": "none",
      "namespaceLayout": "nested",
      "memberLayout": "samePage",
      "EnumSortOrder": "declaringOrder",
      "src": [
        {
          "src": "../",
          "files": [
            "**/*.csproj"
          ],
          "exclude": [
            "**/*.Test.csproj"
          ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "sitemap": {
      "baseUrl": "https://der-floh.github.io/Programs.Manager",
      "changefreq": "monthly"
    },
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern",
      "templates/material"
    ],
    "globalMetadata": {
      "homepage": "docs/introduction.md",
      "pdf": false,
      "_appName": "Program-Infos-Manager",
      "_appFooter": "<strong>Made with DocFX</strong>",
      "_appTitle": "Program-Infos-Manager",
      "_appLogoPath": "images/logo-x64.png",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "_disableContribution": true
    }
  }
}